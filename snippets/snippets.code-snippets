		//setup area
		//////////////////////////////	
		"To configure the DNS domain name, DNS server address": {
			"prefix": ["dnsconfig"],
			"body": [
				"dnsconfig --add -domain ${1:example.com} -serverip1 ${2:8.8.8.8} -serverip2 ${3:8.8.4.4}"
			],
			"description": "Use this command to set the domain name service parameters"
		}
		"To configure the DNS domain name, DNS server address": {
			"prefix": ["dnsconfig"],
			"body": [
				"dnsconfig --add -domain ${1:example.com} -serverip1 ${2:8.8.8.8} -serverip2 ${3:8.8.4.4}"
			],
			"description": "Use this command to set the domain name service parameters"
		}
		"Sets or displays the idle timeout value for a shell or SSH login session": {
			"prefix": ["timeout"],
			"body": [
				"timeout --session ${1:time}"
			],
			"description": "Use this command without an operand to display the current timeout value (in minutes) after which idle logins are automatically terminated"
		}
		"The following example sets the NTP server to a specified address": {
			"prefix": ["tsclockserver"],
			"body": [
				"tsclockserver ${1:NTP_ip}"
			],
			"description": "The tsClockServer command accepts multiple NTP server addresses in IPv4, IPv6, or DNS name formats"
		}
		"Set the switch or chassis name": {
			"prefix": ["switchname,chassisName"],
			"body": [
				"${1|switchname,chassisName|} ${2:name}"
			],
			"description": "Use this command to set the switch or chassis name"
		}
		"Password police overview": {
			"prefix": ["passwdcfg"],
			"body": [
				"passwdcfg --showall"
			],
			"description": "Display the current password configuration parameters"
		}
		"Manages the password policies": {
			"prefix": ["passwdcfg"],
			"body": [
				"passwdcfg --set -uppercase ${1:ABC...} -lowercase ${2:abc...} -digits ${3:123...} -minlength ${4:PW_length} -history ${5:0-24} -maxpasswordage ${6:Indays} -warning ${7:0-999}"
			],
			"description": "Use this command to manage password policies."
		}
		"Add a user account.": {
			"prefix": ["adduser,userconfig"],
			"body": [
				"userconfig --add ${1:username} -l ${2:LF_ID_list(1-128)} -r ${3:role} -c ${4:chassis_role} -p ${5:password}"
			],
			"description": "Use this command to add user accounts on a switch"
		}
		"Delete a user account.": {
			"prefix": ["deleteuser,userconfig"],
			"body": [
				"userconfig --delete ${1:username}"
			],
			"description": "Use this command to delete user accounts on a switch"
		}
		"Sets the Ethernet IP addresses": {
			"prefix": ["ipaddrset"],
			"body": [
				"ipaddrset -ipv4 -add -ethip ${1:ipaddress} -ethmask ${2:mask_ip} -gwyip ${3:gateway_ip} -dhcp ${4|on,off|}"
			],
			"description": "Use this command to configure the IP addresses on a switch"
		}
		"Sets the Ethernet IP addresses for a logical switch": {
			"prefix": ["ipaddrset"],
			"body": [
				"ipaddrset -ls ${1:FID} --add ${2:ipaddress/mask}"
			],
			"description": "Use the following procedure to set up IP addresses for a logical switch"
		}
		"Sets the Ethernet IP addresses for a logical switch": {
			"prefix": ["license"],
			"body": [
				"license --show"
			],
			"description": " Displays license keys\n Old Command: licenseshow"
		}
