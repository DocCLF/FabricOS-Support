	{
		//////////////////////////////
		//Alias area
		//////////////////////////////
		"Creates a zone alias": {
			"prefix": ["alicreate"],
			"body": [
				"alicreate \"${1:AliasName}\" , \"${2:WWPN}\""
			],
			"description": "Create a new Alias"
		}
		"Add a member to a zone alias": {
				"prefix": ["aliadd"],
				"body": [
					"aliadd \"${1:AliasName}\" , \"${2:WWPN}\""
				],
				"description": " Add one or more members to an existing Alias.\nThe alias member list cannot contain another zone alias."
			}
		//
		//////////////////////////////	
		//Zoning area
		//////////////////////////////	
		"Creates a default Zone": {
				"prefix": ["zonecreate"],
				"body": [
					"zonecreate \"${1:ZoneName}\" , \"${2:Member};${3:Member}\""
				],
				"description": "Create a new basic zone"
		}
		"Creates a default Peer Zone": {
				"prefix": ["peerzone","pzone"],
				"body": [
					"zonecreate --peerzone \"${1:ZoneName}\" -principal \"${2:StorageMember}\";\"${3:StorageMember}\" -members \"${4:HostMember}\";\"${5:HostMember}\""
				],
				"description": "Create a new Peer Zone"
		}
		"Adds a Member to the default Zone": {
			"prefix": ["zoneadd"],
			"body": [
				"zoneadd \"${1:ZoneName}\" , \"${2:Member};${3:Member}\""
			],
			"description": "Add one or more members to an existing default zone"
		}
		"Adds a Member AND Principal to the Peer Zone": {
			"prefix": ["peezoneadd"],
			"body": [
				"zoneadd --peerzone \"${1:ZoneName}\" -principal \"${2:StorageMember}\" -members \"${3:HostMember}\""
			],
			"description": "Add one or more Member AND Principal to an existing Peer Zone"
		}		
		"Adds a Member OR Principal to the Peer Zone": {
				"prefix": ["peerzoneadd"],
				"body": [
					"zoneadd --peerzone \"${1:ZoneName}\" ${2|-principal,-members|} \"${3:Member}\""
				],
				"description": "Add one or more Member OR Principal to an existing Peer Zone"
		}
		"Removes a member from a default zone": {
			"prefix": ["zoneremove"],
			"body": [
				"zoneremove \"${1:ZoneName}\" , \"${2:Member};${3:Member}\""
			],
			"description": "Remove one or more members from an existing zone"
		}
		"Removes a member AND principal (Peer Zone) from a zone": {
			"prefix": ["peerzoneremove"],
			"body": [
				"zoneremove --peerzone \"${1:ZoneName}\" -principal \"${2:StorageMember}\" -members \"${3:HostMember}\""
			],
			"description": "Remove one or more Member AND Principal members from a Peer Zone"
		}	
		"Removes a member OR principal (Peer Zone) from a zone": {
			"prefix": ["peerzoneremove"],
			"body": [
				"zoneremove --peerzone \"${1:ZoneName}\" ${2|-principal,-members|} \"${3:Alias}\""
			],
			"description": "Remove one or more Member OR Principal members from a Peer Zone"
		}
		"Show only Peer Zones": {
			"prefix": ["peerzoneshow"],
			"body": [
				"zoneshow --peerzone -all"
			],
			"description": "List all Peer Zones in the zone database"
		}	
		"Show all Zones": {
			"prefix": ["zoneshow"],
			"body": [
				"zoneshow"
			],
			"description": "List all Zones in the zone database"
		}	
		"Renames a zone object": {
			"prefix": ["zoneObjectRename"],
			"body": [
				"zoneObjectRename \"${1:oldName}\" , \"${2:newName}"
			],
			"description": "Rename a zone object. You can use this command for all zone object types, including cfg, zone, and alias."
		}
		"Copies a zone object": {
			"prefix": ["zoneObjectCopy"],
			"body": [
				"zoneObjectCopy \"${1:oldName}\" , \"${2:newName}"
			],
			"description": "Make a copy of an existing zone object and give it a new name. The resulting object is of the same type as the original object. You can use this command for all zone object types, including cfg, zone, and alias."
		}	
