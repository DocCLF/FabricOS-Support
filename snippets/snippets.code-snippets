		"Configures a port to support long distance links": {
			"prefix": ["portcfglongdistance"],
			"body": [
				"portcfglongdistance ${1:port} ${2|L0,LE,LD,LS|} 1 -distance ${3:distance} -framesize ${4:frame_size}"
			],
			"description": "Use this command to allocate frame buffer credits to a port or to configure a specified long distance link. The port can only be used as an E_Port."
		}
		"Configures normal distance E_Port buffer allocation": {
			"prefix": ["portcfgeportcredits"],
			"body": [
				"portcfgeportcredits --${1|enable,disable,show|} ${2:port} ${3:credits_in_use_with_enable}"
			],
			"description": "Use this command to change the default credit allocation for a normal distance E_Port or EX_Port by allocating the specified number of credits to the port."
		}
